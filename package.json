{
  "name": "lojas-pokemon",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "start": "node --max_old_space_size=4096 scripts/start.js",
    "build": "node --max_old_space_size=4096 scripts/build.js",
    "test": "cypress open",
    "lint": "eslint \"src/**/*.{js,jsx}\" --max-warnings 0"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.19.2",
    "eslint-config-prettier": "^6.15.0",
    "history": "^4.10.1",
    "react": "^16.8.6",
    "react-dom": "16.8.6",
    "react-icons": "^4.2.0",
    "react-redux": "6.0.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "4.0.1"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^1.1.3",
    "autoprefixer": "9.4.4",
    "babel-core": "6.26.3",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "7.1.5",
    "babel-preset-react-app": "3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.2",
    "css-loader": "0.28.7",
    "cypress": "^4.12.1",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-react-app": "3.0.6",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "3.2.0",
    "husky": "^3.1.0",
    "jest": "23.6.0",
    "jest-transform-stub": "^2.0.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "promise": "8.0.2",
    "raf": "3.4.1",
    "react-dev-utils": "5.0.1",
    "semantic-release": "^17.3.0",
    "shelljs": "^0.8.4",
    "source-map-explorer": "^2.5.2",
    "style-loader": "0.23.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "url-loader": "1.1.2",
    "webpack": "^3.12.0",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "3.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "!src/**/index.{js,jsx,mjs}",
      "!src/**/redux.{js,jsx,mjs}",
      "!src/**/propTypes.{js,jsx,mjs}",
      "!src/**/style.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub"
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "jest-transform-stub"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ],
    "modulePaths": [
      "src"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "env": {
      "development": {
        "compact": false
      }
    }
  }
}
